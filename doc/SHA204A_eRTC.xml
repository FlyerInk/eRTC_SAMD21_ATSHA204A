      <Number>1</Number>
      <Config>80 80</Config>
      <UseFlag>FF</UseFlag>
      <UpdateCount>00</UpdateCount>
      <Mode>Secret</Mode>
      <HostTargetKey></HostTargetKey>
      <SnPad></SnPad>
